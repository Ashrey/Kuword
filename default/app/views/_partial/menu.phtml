<nav class="navbar ">
	 <div class="navbar-inner">
		{% exec Html::link '' title 'class="brand"' %}
		<ul class="nav">
		{% for i in menu  %}
			{%exec json_decode i.metadata as meta%}
			{% buffer icon%}
				{%if meta.icon %}<i class="icon-{{meta.icon}} icon-2x"></i>{{i.showed_text}}{%endif%}
			{%endbuffer%}
			<li>{% exec Html::link i.href, icon %}</li>
		{% endfor%}
		</ul>
	</div>
</nav>
